{
    "name": "MailerService",
    "version": "1.0.0",
    "description": "Backend of TFG Project using nestjs",
    "author": "Arturo Alca√±iz",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start": "ts-node --files src/main",
        "clean": "rm -Rf ./dist/",
        "build:ci": "npm run build",
        "build": "npm run clean && mkdir ./dist && tsc --project tsconfig.build.json",
        "prettier": "prettier --write {,*/**/}*.{ts,js,jsx,json}",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --silent=false",
        "lint": "npx eslint -c .eslintrc.js --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern"
    },
    "dependencies": {
        "@nestjs/axios": "^0.0.8",
        "@nestjs/common": "^8.4.6",
        "@nestjs/config": "^2.1.0",
        "@nestjs/core": "^8.4.6",
        "@nestjs/jwt": "^8.0.1",
        "@nestjs/mapped-types": "^1.0.0",
        "@nestjs/microservices": "^8.0.0",
        "@nestjs/platform-express": "^8.4.6",
        "@nestjs/swagger": "^5.0.8",
        "@nestjs/throttler": "^2.0.1",
        "@nestjs/typeorm": "^8.1.2",
        "@nestjs/websockets": "^8.4.6",
        "@types/multer-s3": "^2.7.11",
        "amqp-connection-manager": "^4.1.3",
        "amqplib": "^0.9.1",
        "apollo-server-express": "^3.8.1",
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "bufferutil": "^4.0.6",
        "cache-manager": "^4.0.0",
        "canvas": "^2.9.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cookie-parser": "^1.4.6",
        "express": "^4.18.1",
        "fastify-swagger": "^5.2.0",
        "graphql": "^15.4.0",
        "handlebars": "^4.7.7",
        "helmet": "^5.0.2",
        "kafkajs": "^1.15.0",
        "mapped-types": "^0.0.1",
        "mariadb": "^2.5.4",
        "mqtt": "^4.2.8",
        "multer-s3": "^3.0.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.3.3",
        "nats": "^2.7.1",
        "neo4j-driver": "^4.4.5",
        "nest-winston": "^1.6.2",
        "nestjs-form-data": "^1.6.3",
        "nodemailer": "^6.7.5",
        "passport-jwt": "^4.0.0",
        "redis": "^4.1.0",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.2",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.5",
        "swagger-ui-express": "^4.4.0",
        "synchronized-promise": "^0.3.1",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.6",
        "utf-8-validate": "^5.0.9",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@darraghor/eslint-plugin-nestjs-typed": "^3.10.1",
        "@nestjs/cli": "^8.2.6",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^8.4.6",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.13",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.38",
        "@types/nodemailer": "^6.4.4",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/stripe": "^8.0.417",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8.3.4",
        "prettier": "^2.6.2",
        "supertest": "^6.2.3",
        "ts-jest": "28.0.3",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.2"
    },
    "overrides": {
        "@nestjs/platform-express": {
            "multer": "^1.4.5-lts.1"
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
